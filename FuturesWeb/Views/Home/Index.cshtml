@model Dictionary<string, List<Futures.Model.LabelToUpdate>>
@{
    ViewBag.Title = "Home Page";
}

<div class="container">
    <div class="row">
        <div class="col-md-2 offset-md-2">
            <div class="row">
                <div class="col-md-12">Enter cumulative</div>
                <div class="col-md-12">15000</div>
            </div>
        </div>
        <div class="col-md-2">
            This week
        </div>
        <div class="col-md-2">
            Next week
        </div>
        <div class="col-md-2 offset-md-2">
            Quarter
        </div>
    </div>

    @foreach (var labelRow in Model)
    {
        <div class="row">
            <div class="col-md-2 offset-md-2">
                <h2>@labelRow.Key</h2>
            </div>
            @foreach (var label in labelRow.Value)
            {
                <div class="col-md-2">
                    @label.Content
                </div>
            }
        </div>
    }
</div>